<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.23.xsd">

    <changeSet id="002-add-brand-model-to-logistics" author="TicBnPickTeam">
        <validCheckSum>ANY</validCheckSum>
        <preConditions onFail="MARK_RAN">
            <not>
                <columnExists tableName="logistics" columnName="brand"/>
            </not>
        </preConditions>
        <addColumn tableName="logistics">
            <column name="brand" type="varchar(255)"/>
        </addColumn>
    </changeSet>

    <changeSet id="002b-add-model-to-logistics" author="TicBnPickTeam">
        <validCheckSum>ANY</validCheckSum>
        <preConditions onFail="MARK_RAN">
            <not>
                <columnExists tableName="logistics" columnName="model"/>
            </not>
        </preConditions>
        <addColumn tableName="logistics">
            <column name="model" type="varchar(255)"/>
        </addColumn>
    </changeSet>

</databaseChangeLog>
